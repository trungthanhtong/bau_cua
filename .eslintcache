 </Dropdown>\n                    <Heading3>To do list</Heading3>\n                    <TextField\n                        className=\"w-50\"\n                        label=\"Task name: \"\n                        value={this.state.taskName}\n                        onChange={(e) => {\n                            this.setState({\n                                taskName: e.target.value,\n                            });\n                        }}\n                    ></TextField>\n\n                    {this.state.disabled ? (\n                        <span>\n                            <Button\n                                className=\"ms-2\"\n                                onClick={() => {\n                                    let { taskName } = this.state;\n                                    let task = {\n                                        id: Date.now(),\n                                        taskName: taskName,\n                                        done: false,\n                                    };\n                                    this.props.dispatch(addTaskAction(task));\n                                }}\n                            >\n                                <i className=\"fa fa-plus\"></i> Add task\n                            </Button>\n                            <Button\n                                disabled\n                                className=\"ms-2\"\n                                onClick={() => {\n                                    this.props.dispatch(\n                                        updateTask(this.state.taskName)\n                                    );\n                                }}\n                            >\n                                <i className=\"fa fa-upload\"></i> Update Task\n                            </Button>\n                        </span>\n                    ) : (\n                        <span>\n                            <Button\n                                disabled\n                                className=\"ms-2\"\n                                onClick={() => {\n                                    let { taskName } = this.state;\n                                    let task = {\n                                        id: Date.now(),\n                                        taskName: taskName,\n                                        done: false,\n                                    };\n                                    this.props.dispatch(addTaskAction(task));\n                                }}\n                            >\n                                <i className=\"fa fa-plus\"></i> Add task\n                            </Button>\n                            <Button\n                                className=\"ms-2\"\n                                onClick={() => {\n                                    this.props.dispatch(\n                                        updateTask(this.state.taskName)\n                                    );\n                                }}\n                            >\n                                <i className=\"fa fa-upload\"></i> Update Task\n                            </Button>\n                        </span>\n                    )}\n\n                    <br />\n                    <Heading3>Task to do</Heading3>\n                    <Table>\n                        <Thead>{this.renderTaskToDo()}</Thead>\n                    </Table>\n                    <Heading3>Task complete</Heading3>\n                    <Table>\n                        <Thead>{this.renderTaskDone()}</Thead>\n                    </Table>\n                </Container>\n            </ThemeProvider>\n        );\n    }\n\n    componentDidUpdate(prevProps, prevState) {\n        console.log(\"prev\", prevProps);\n        console.log(\"prop\", this.props);\n        if (this.props.taskEdit.id !== prevProps.taskEdit.id) {\n            this.setState({\n                taskName: this.props.taskEdit.taskName,\n            });\n        }\n    }\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        themeToDoList: state.ToDoListReducer.themeToDoList,\n        taskList: state.ToDoListReducer.taskList,\n        taskEdit: state.ToDoListReducer.taskEdit,\n    };\n};\n\nexport default connect(mapStateToProps)(ToDoList);\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Container/Container.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Theme/ToDoListDarkTheme.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Theme/ToDoListLightTheme.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Components/Dropdown.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Components/Heading.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/JSS/Components/Table.js",[],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/redux/reducers/rootReducer.js",["267","268"],"/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/redux/reducers/ToDoListReducer.js",["269"],"import { ToDoListDarkTheme } from \"../../JSS/Theme/ToDoListDarkTheme\"\nimport { add_task, change_theme, delete_task, done_task, edit_task, update_task } from \"../types/ToDoListTypes\"\nimport {arrTheme} from '../../JSS/Theme/ThemeManager'\n\n/* eslint-disable import/no-anonymous-default-export */\nconst initialState = {\n    themeToDoList: ToDoListDarkTheme,\n    taskList: [\n        {id:'task-1', taskName:'task 1', done:false},\n        {id:'task-2', taskName:'task 2', done:true},\n        {id:'task-3', taskName:'task 3', done:false},\n    ],\n    taskEdit: {}\n}\n\nexport default (state = initialState, action) => {\n    switch (action.type) {\n        case add_task: {\n            if (action.newTask.taskName.trim() === '') {\n                alert('Task is empty!')\n                return {...state}\n            }\n            let taskListUpdate = [...state.taskList];\n            // console.log(taskListUpdate);\n            state.taskList = [...taskListUpdate, action.newTask];\n            console.log(taskListUpdate);\n            return {...state}\n        }\n\n        case change_theme: {\n            let index = arrTheme.findIndex(item => item.id == action.themeID);\n            if (index !== -1) {\n                state.themeToDoList = arrTheme[index].theme;\n            }\n            return {...state}\n        }\n\n        case done_task: {\n            let taskListUpdate = [...state.taskList];\n            let index = taskListUpdate.findIndex(item => item.id === action.taskID) ;\n            if (index !== -1) {\n                taskListUpdate[index].done = true;\n            }           \n            // state.taskList = [...taskListUpdate]\n            return {...state, taskList: taskListUpdate}\n        }\n\n        case delete_task: {\n            // let taskListUpdate = [...state.taskList];\n            // let index = taskListUpdate.findIndex(item => item.id === action.taskDeleteID);\n            // if (index !== -1) {\n            //     taskListUpdate.slice(index, 1);\n            // }\n            // taskListUpdate = taskListUpdate.filter(item => item.id !== action.taskID);\n            return {...state, taskList: state.taskList.filter(item => item.id !== action.taskID)};\n        }\n\n        case edit_task: {\n            return {...state, taskEdit: action.task}\n        }\n\n        case update_task: {\n            state.taskEdit = {...state.taskEdit, taskName: action.taskName}\n            let taskListUpdate = state.taskList;\n            let index = taskListUpdate.findIndex(task => task.id === state.taskEdit.id);\n            if (index !== -1) {\n                taskListUpdate[index].taskName = action.taskName;\n            }\n            return {...state, taskList: taskListUpdate}\n        }\n        default: return {...state}\n    }\n\