log('newItem', newItem)\n                // cartUpdate.push(newItem);\n            }\n            return cartUpdate;\n        }\n        return [...state]\n    }\n};\n\nlet arrProduct = [\n    { id: 1, name: \"iphone\", price: 1000 },\n    { id: 2, name: \"Note10\", price: 3000 },\n    { id: 3, name: \"Sony\", price: 2000 },\n];\n\nexport default function DemoUseReducer() {\n    let [cart, dispatch] = useReducer(cartReducer, initialCart);\n\n    const addToCart = (itemClick) => {\n        dispatch({\n            type: 'addToCart',\n            item: itemClick\n        })\n    }\n\n    console.log('cart', cart)\n\n    return (\n        <div className=\"container\">\n            <div className=\"row\">\n                {arrProduct.map((item, index) => {\n                    return (\n                        <div className=\"col-4\" key={index}>\n                            <div className=\"card text-dark bg-white\">\n                                <img\n                                    className=\"card-img-top\"\n                                    src=\"https://picsum.photos/200/200\"\n                                    alt='photo'\n                                    style={{width:'100%'}}\n                                />\n                                <div className=\"card-body\">\n                                    <h4 className=\"card-title\">{item.name}</h4>\n                                    <p className=\"card-text\">{item.price}</p>\n                                    <button onClick={() => {addToCart(item)}} className=\"btn btn-success\">Add to cart</button>\n                                </div>\n                            </div>\n                        </div>\n                    );\n                })}\n            </div>\n\n            <h3>Giỏ hàng</h3>\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th>id</th>\n                        <th>name</th>\n                        <th>price</th>\n                        <th>quantity</th>\n                        <th>total</th>\n                        <th></th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {cart.map((product, index) => {\n                        return (\n                            <tr key={index}>\n                                <td>{product.id}</td>\n                                <td>{product.name}</td>\n                                <td>{product.price}</td>\n                                <td>{product.quantity}</td>\n                                <td>{product.quantity * product.price}</td>\n                                <td>\n                                    <button className=\"btn btn-danger\">\n                                        X\n                                    </button>\n                                </td>\n                            </tr>\n                        );\n                    })}\n                </tbody>\n            </table>\n        </div>\n    );\n}\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/Hooks/DemoUseContext.js",["285"],"import React, { useContext } from \"react\";\nimport { context } from \"./Context/ContextProvider\";\n\nconst arrProduct = [\n    { id: 1, name: \"iphone\", price: 1000 },\n    { id: 2, name: \"Note10\", price: 3000 },\n    { id: 3, name: \"Sony\", price: 2000 },\n];\nexport default function DemoUseContext() {\n    // let store = useContext(context)\n    // let cartReducer = store.cartReducer\n\n    let { cartReducer } = useContext(context);\n    let [cart, dispatch] = cartReducer;\n\n    const addToCart = (item) => {\n        dispatch({\n            type: 'addToCart',\n            item\n        })\n    }\n\n    console.log(dispatch);\n    return (\n        <div className=\"container\">\n            <div className=\"row\">\n                {arrProduct.map((item, index) => {\n                    return (\n                        <div className=\"col-4\" key={index}>\n                            <div className=\"card text-dark bg-white\">\n                                <img\n                                    className=\"card-img-top\"\n                                    src=\"https://picsum.photos/200/200\"\n                                    alt=\"photo\"\n                                    style={{ width: \"100%\" }}\n                                />\n                                <div className=\"card-body\">\n                                    <h4 className=\"card-title\">{item.name}</h4>\n                                    <p className=\"card-text\">{item.price}</p>\n                                    <button\n                                        onClick={() => {\n                                            addToCart(item);\n                                        }}\n                                        className=\"btn btn-success\"\n                                    >\n                                        Add to cart\n                                    </button>\n                                </div>\n                            </div>\n                        </div>\n                    );\n                })}\n            </div>\n\n            <h3>Giỏ hàng</h3>\n            <table className=\"table\">\n                <thead>\n                    <tr>\n                        <th>id</th>\n                        <th>name</th>\n                        <th>price</th>\n                        <th>quantity</th>\n                        <th>total</th>\n                        <th></th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {cart.map((product, index) => {\n                        return (\n                            <tr key={index}>\n                                <td>{product.id}</td>\n                                <td>{product.name}</td>\n                                <td>{product.price}</td>\n                                <td>{product.quantity}</td>\n                                <td>{product.quantity * product.price}</td>\n                                <td>\n                                    <button className=\"btn btn-danger\">\n                                        X\n                                    </button>\n                                </td>\n                            </tr>\n                        );\n                    })}\n                </tbody>\n            </table>\n        </div>\n    );\n}\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/Hooks/Context/ContextProvider.js",["286","287"],"import React, { useReducer } from 'react'\n\nexport const context = React.createContext()\n\nconst initialCart = [{ id: 1, name: \"iphone\", price: 1000, quantity: 1 }];\n\nconst cartReducer = (state, action) => {\n    switch (action.type) {\n        case 'addToCart': {\n            let cartUpdate = [...state];\n            let index = cartUpdate.findIndex(item => item.id === action.item.id);\n            if (index !== -1) {\n                cartUpdate[index].quantity += 1;\n            } else {\n                cartUpdate.push({...action.item, quantity: 1})\n                // let newItem = {...action.item, quantity: 1};\n                // console.log('newItem', newItem)\n                // cartUpdate.push(newItem);\n            }\n            return cartUpdate;\n        }\n        return [...state]\n    }\n};\n\nexport default function ContextProvider(props) {\n    let [cart, dispatch] = useReducer(cartReducer, initialCart)\n\n    let store = {\n        cartReducer: [cart, dispatch]\n    }\n\n    // Dùng context bao bọc tất cả components bên trong\n    return (\n        <context.Provider value={store}>\n            {props.children}\n        </context.Provider>\n    )\n}\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/Hooks/DemoReduxApp.js",["288"],"import React, { useState } from 'react'\nimport {connect, useDispatch, useSelector} from 'react-redux'\n\nexport default function DemoReduxApp(props) {\n\n    let comments = useSelector(state => state.FakebookReducer.comments)\n    let dispatch = useDispatch();\n\n    let [userComment, setUserComment] = useState({\n        name: '',\n        content: '',\n        avatar: ''\n    })\n\n    const handleChange = (e) => {\n        let {value, name} = e.target;\n        setUserComment({\n            ...userComment, [name]: value\n        })\n    }\n\n    const handleComment = (e) => {\n        e.preventDefault();\n        let comment = {...userComment, avatar: `https://i.pravatar.cc/150?u=${userComment.name}`}\n        dispatch({\n            type: 'add_comment',\n            comment\n        })\n    }\n\n    return (\n        <div className=\"container py-5\">\n            <h3>Fakebook App</h3>\n            <div className=\"card text-dark text-start\">\n                <div className=\"card-header\">\n                    {/* <div className=\"row\">\n                        <div className=\"col-1\">\n                            <img style={{height:'50px'}} src=\"https://picsum.photos/200/200\" alt=\"picsum\"/>\n                        </div>\n                        <div className=\"col-11\">\n                            <h6 className=\"text-danger\">Yone</h6>\n                            <p>Hia</p>\n                        </div>\n                    </div> */}\n                    {comments.map((comment, index) => {\n                        return (\n                            <div className=\"row\" key={index}>\n                            <div className=\"col-1\">\n                                <img style={{height:'50px'}} src={comment.avatar} alt=\"picsum\"/>\n                            </div>\n                            <div className=\"col-11\">\n                                <h6 className=\"text-danger\">{comment.name}</h6>\n                                <p>{comment.content}</p>\n                            </div>\n                        </div>\n                        )\n                    })}\n                </div>\n              <form onSubmit={handleComment} class=\"card-body\">\n                  <div className=\"form-group\">\n                      <h6 className=\"card-text\">Name</h6>\n                      <input value={userComment.name} onChange={handleChange} type=\"text\" name='name' className=\"form-control\"/>\n                  </div>\n                  <div className=\"form-group\">\n                      <h6 className=\"card-text\">Content</h6>\n                      <input value={userComment.content} onChange={handleChange} type=\"text\" name='content' className=\"form-control\"/>\n                  </div>\n                  <div className=\"form-group\">\n                      <button className=\"btn btn-success mt-3\">Send</button>\n                  </div>\n              </form>\n            </div>\n        </div>\n    )\n}\n\n// const mapStateToProps = state => {\n//     return {\n//         comments : state.FakebookReducer.comments\n//     }\n// }\n\n// export default connect(mapStateToProps)(DemoReduxApp);\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC/chapter3/src/redux/reducers/FakebookReducer.js",["289"],"const initialState = {\n    comments: [\n        {\n            name: \"Yone\",\n            content: \"Hi\",\n            avatar: `https://i.pravatar.cc/150?u=yone`,\n        },\n        {\n            name: \"Yasuo\",\n            content: \"Hi man\",\n            avatar: `https://i.pravatar.cc/150?u=yasuo`,\n        },\n    ],\n};\n\nconst FakebookReducer = (state = initialState, action) => {\n    switch (action.type) {\n        case \"add_comment\": {\n            state.comments = [...state.comments, action.comment]\n            return {...state}\n        }\n    }\n    return { ...state };\n};\n\nexport default FakebookReducer;\n","/Users/thanhtong/Desktop/CyberSoft/React/Demo/Chapter3_RFC